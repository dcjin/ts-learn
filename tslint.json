{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "rules": {
    "ter-indent": [
      true,
      4,
      {
        "VariableDeclarator": 1
      }
    ],
    "trailing-comma": [
      false
    ],
    "import-name": [
      true,
      {
        "underscore": "_"
      }
    ]
  },
  "rulesDirectory": []
}